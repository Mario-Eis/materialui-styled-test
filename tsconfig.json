{
  "compilerOptions": {
    "outDir": "out/dist",
    "module": "esnext",
    "target": "es5",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": false
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}